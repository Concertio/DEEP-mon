{
  "version": 1,
  "schedule": {
    "type": "streaming"
  },
  "max-failures": 10,
  "workflow": {
    "collect": {
      "metrics": {
        "/hyppo/hyppo-monitor/*/*/*/*/container_name": {}
      },
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/container-namer.json"
          }
        }
      ]
    }
  }
}
